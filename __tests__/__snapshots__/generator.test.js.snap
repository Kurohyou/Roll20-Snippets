        attributes[\`\${section}_\${id}_\${field}\`] = \`%{\${attributes.character_name}|\${section}_\${id}_\${fieldAction}}\`;
      });
    }else{
      attributes[\`\${field}\`] = \`%{\${attributes.character_name}|\${fieldAction}}\`;
