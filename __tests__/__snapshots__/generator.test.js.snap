 */
const debug = function(msg,force){
  if(!kFuncs.debugMode && !force && kFuncs.version > 0) return;
  if(typeof msg === 'string'){
    console.log(\`%c\${kFuncs.sheetName} DEBUG| \${msg}\`,\\"background-color:tan;color:red;\\");
  }else if(typeof msg === 'object'){
